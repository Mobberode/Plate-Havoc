{
    "type": "multi_action",
    "title": "Plate Havoc Console",
    "columns": 1,
    "exit_action": {"label":"Back","action":{"type":"show_dialog","dialog":"plate_havoc:console"}},
    "inputs": [{"key":"id","type":"text","label":"Event ID"}],
    
    "actions": [
        {"label":"Force Event","action":{"type":"dynamic/run_command","template":"function plate_havoc:console/force_event {id:$(id)}"}},
        {"label":"Force Random Event","action":{"type":"run_command","command":"function plate_havoc:game/events/behaviour"}}
    ]
}